<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kendy.db.dao.PlayerDao">
  <resultMap id="BaseResultMap" type="com.kendy.db.entity.Player">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="playerId" jdbcType="VARCHAR" property="playerid" />
    <result column="playerName" jdbcType="VARCHAR" property="playername" />
    <result column="gudong" jdbcType="VARCHAR" property="gudong" />
    <result column="teamId" jdbcType="VARCHAR" property="teamid" />
    <result column="edu" jdbcType="VARCHAR" property="edu" />
    <result column="isParent" jdbcType="VARCHAR" property="isparent" />
    <result column="huibao" jdbcType="VARCHAR" property="huibao" />
    <result column="huishui" jdbcType="VARCHAR" property="huishui" />
  </resultMap>

  <insert id="insertBatch">
    INSERT INTO members
    (playerId, playerName, gudong, teamId, edu, isParent, huibao, huishui)
    VALUES
    <foreach collection ="list" item="p" separator =",">
      (#{p.playerid}, #{p.playername}, #{p.gudong}, #{p.teamid}, #{p.edu}, #{p.isparent}, #{p.huibao}, #{p.huishui})
    </foreach >
  </insert>
</mapper>